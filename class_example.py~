class Protein():
    protein_id=''
    protein_sequence=''
    def __init__ (self, id, sequence):
        self.protein_id = id
        self.protein_sequence = sequence
    def aminocount (self, aminoacid):
        aacount=0
        for a in self.protein_sequence:
            inf a ==aminoacid:
                aacount=aacount+1
            return aacount